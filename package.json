{
  "name": "git-cli",
  "description": "Cli app to fetch e query programming languages and locations",
  "license": "MIT",
  "author": "Leonardo Lopes",
  "version": "0.0.12",
  "main": "/bin/src/index.js",
  "bin": {
    "git-cli": "./bin/src/index.js"
  },
  "scripts": {
    "clean": "rimraf bin/",
    "copy-files": "copyfiles -u 1 db/**/*.sql src/**/*.css bin/db",
    "build": "npm run clean && tsc -p .&& npm run copy-files",
    "audit": "lerna run audit --stream",
    "audit:depcheck": "lerna run audit:depcheck --stream",
    "audit:npm-check-updates": "npx npm-check-updates --errorLevel 1",
    "create": "npm run build && npm run test",
    "local": "sudo npm uninstall -g && sudo npm install -g && git-api",
    "npm:format": "npx prettier-package-json --write package.json",
    "npm:upgrade": "npx npm-check-updates -iu",
    "refresh": "rm -rf ./node_modules ./package-lock.json && npm install",
    "start": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "test": "sudo npm i -g && git-api",
    "lint": "./node_modules/.bin/eslint --ext .js --ignore-path .gitignore ."
  },
  "dependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/express": "^4.17.13",
    "axios": "^0.27.2",
    "chalk": "^4.1.2",
    "clear": "^0.1.0",
    "commander": "^7.2.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "dotenv": "^16.0.1",
    "path": "^0.12.7",
    "pg-monitor": "^1.4.1",
    "pg-promise": "^10.11.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/clear": "^0.1.2",
    "@types/figlet": "^1.5.4",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.18.22",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "bluebird": "^3.7.2",
    "cli": "^1.0.1",
    "copyfiles": "^2.4.1",
    "depcheck": "^1.1.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.4",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^6.0.0",
    "express": "^4.18.1",
    "figlet": "^1.5.2",
    "lerna": "^3.1.4",
    "mkdirp": "^0.5.1",
    "nodemon": "^1.4.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-cli": "^4.10.0"
  },
  "keywords": [
    "git-cli",
    "nodejs",
    "git",
    "typescript"
  ],
  "directories": {
    "lib": "lib",
    "bin": "./bin"
  }
}
